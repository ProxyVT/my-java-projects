import java.util.Scanner;
 
public class Age {
   
    public static void main(String[] args) {
           
        Scanner in = new Scanner(System.in);
        System.out.print("Please enter your age: ");
        int a = in.nextInt();
		int years = a;
		int months = years * 12;
		int weeks = months * 54;
		int days = weeks * 365;
		long hours = days * 8760L;
		long minutes = hours * 525600L;
		double seconds = minutes * 31536000L;
		double milliseconds = seconds * 31536000000L;
        
		System.out.printf("Your age in years: %d \n", years);
        System.out.printf("Your age in months: %d \n", months);
        System.out.printf("Your age in weeks: %d \n", weeks);
        System.out.printf("Your age in days: %d \n", days);
        System.out.printf("Your age in hours: %d \n", hours);
        System.out.printf("Your age in minutes: %d \n", minutes);
        System.out.printf("Your age in seconds: %e \n", seconds);
        System.out.printf("Your age in milliseconds: %e \n", milliseconds);
        in.close();
    }
}
